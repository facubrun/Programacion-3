matriz_t max_datos_procesados(nat n, nat* datos, nat* rendimiento) {
	matriz_t OPT = crear_matriz(n+1, n+1);
	for(nat i=0;i<=n;i++){
		for(nat j=0;j<=n;j++){
			if(i<j){
				OPT[i][j]=0;
			}
			else{
				nat rend_acum = rend_acum + rendimiento[i];
				
				
				
			
	return OPT;
}

